<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="admin/tool/outcome/db" VERSION="20130819" COMMENT="XMLDB file for Moodle admin/tool/outcome"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="outcome_sets" COMMENT="An outcome set represents a collection of outcomes and perhaps some meta data regarding the revision or applicable region." NEXT="outcome">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="idnumber"/>
        <FIELD NAME="idnumber" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="GUID that is auto-generated if missing." PREVIOUS="id" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="The name of the outcome set." PREVIOUS="idnumber" NEXT="description"/>
        <FIELD NAME="description" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="The description of the outcome." PREVIOUS="name" NEXT="provider"/>
        <FIELD NAME="provider" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="Provider information regarding the outcome set. EG: Could be set to ASN." PREVIOUS="description" NEXT="revision"/>
        <FIELD NAME="revision" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="Version information regarding the outcome set. This will typically be a date or year, EG: 2011" PREVIOUS="provider" NEXT="region"/>
        <FIELD NAME="region" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="Any region information. Example: In the US this could be a state name." PREVIOUS="revision" NEXT="deleted"/>
        <FIELD NAME="deleted" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="If the outcome set has been deleted." PREVIOUS="region" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Time modified." PREVIOUS="deleted" NEXT="timecreated"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Time created." PREVIOUS="timemodified"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="idnumber" UNIQUE="true" FIELDS="idnumber"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="outcome" COMMENT="Stores information regarding an individual outcome. Outcomes can be stored in a hierarchy, but the hierarchy is visual only." PREVIOUS="outcome_sets" NEXT="outcome_metadata">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="outcomesetid"/>
        <FIELD NAME="outcomesetid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="parentid"/>
        <FIELD NAME="parentid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Self referencing key. Identifies the parent outcome in the hierarchy." PREVIOUS="outcomesetid" NEXT="idnumber"/>
        <FIELD NAME="idnumber" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="GUID that is auto-generated if missing." PREVIOUS="parentid" NEXT="docnum"/>
        <FIELD NAME="docnum" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="Field for tracking additional information on the outcome. EG: 2.3.A" PREVIOUS="idnumber" NEXT="description"/>
        <FIELD NAME="description" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="The description of the outcome." PREVIOUS="docnum" NEXT="assessable"/>
        <FIELD NAME="assessable" TYPE="int" LENGTH="1" NOTNULL="true" SEQUENCE="false" COMMENT="If set to 1, then the outcome is assessable and can be mapped to content. If set to 0, then the outcome is not assessable and cannot be mapped to content." PREVIOUS="description" NEXT="deleted"/>
        <FIELD NAME="deleted" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="If the outcome has been deleted." PREVIOUS="assessable" NEXT="sortorder"/>
        <FIELD NAME="sortorder" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="The sort order. This is 0-X for all outcomes that belong to the same outcome set." PREVIOUS="deleted" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Time modified." PREVIOUS="sortorder" NEXT="timecreated"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Time created." PREVIOUS="timemodified"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="outcomesetid"/>
        <KEY NAME="outcomesetid" TYPE="foreign" FIELDS="outcomesetid" REFTABLE="outcome_sets" REFFIELDS="id" PREVIOUS="primary" NEXT="parentid"/>
        <KEY NAME="parentid" TYPE="foreign" FIELDS="parentid" REFTABLE="outcome" REFFIELDS="id" PREVIOUS="outcomesetid"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="idnumber" UNIQUE="true" FIELDS="idnumber"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="outcome_metadata" COMMENT="Represents additional meta information regarding outcomes." PREVIOUS="outcome" NEXT="outcome_areas">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="outcomeid"/>
        <FIELD NAME="outcomeid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="The name of the meta data." PREVIOUS="outcomeid" NEXT="value"/>
        <FIELD NAME="value" TYPE="char" LENGTH="1333" NOTNULL="false" SEQUENCE="false" COMMENT="The value of the meta data." PREVIOUS="name"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="outcomeid"/>
        <KEY NAME="outcomeid" TYPE="foreign" FIELDS="outcomeid" REFTABLE="outcome" REFFIELDS="id" PREVIOUS="primary"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="outcome_areas" COMMENT="This represents that a content item has been mapped to an outcome or to multiple outcomes." PREVIOUS="outcome_metadata" NEXT="outcome_area_outcomes">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="component"/>
        <FIELD NAME="component" TYPE="char" LENGTH="100" NOTNULL="true" SEQUENCE="false" COMMENT="The name of the component." PREVIOUS="id" NEXT="area"/>
        <FIELD NAME="area" TYPE="char" LENGTH="50" NOTNULL="true" SEQUENCE="false" COMMENT="The name of the area. Used when a component has multiple items to associate to outcomes." PREVIOUS="component" NEXT="itemid"/>
        <FIELD NAME="itemid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="The item ID, usually an ID from another table." PREVIOUS="area"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="component_area_itemid" UNIQUE="true" FIELDS="component, area, itemid"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="outcome_area_outcomes" COMMENT="This represents the one to many relationship of a single content item mapped to many outcomes." PREVIOUS="outcome_areas" NEXT="outcome_used_areas">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="outcomeid"/>
        <FIELD NAME="outcomeid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="outcomeareaid"/>
        <FIELD NAME="outcomeareaid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="outcomeid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="outcomeid"/>
        <KEY NAME="outcomeid" TYPE="foreign" FIELDS="outcomeid" REFTABLE="outcome" REFFIELDS="id" PREVIOUS="primary" NEXT="outcomeareaid"/>
        <KEY NAME="outcomeareaid" TYPE="foreign" FIELDS="outcomeareaid" REFTABLE="outcome_areas" REFFIELDS="id" PREVIOUS="outcomeid"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="outcome_used_areas" COMMENT="Associates an outcome content item with an activity." PREVIOUS="outcome_area_outcomes" NEXT="outcome_used_sets">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="cmid"/>
        <FIELD NAME="cmid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="outcomeareaid"/>
        <FIELD NAME="outcomeareaid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="cmid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="cmid"/>
        <KEY NAME="cmid" TYPE="foreign" FIELDS="cmid" REFTABLE="course_modules" REFFIELDS="id" PREVIOUS="primary" NEXT="outcomeareaid"/>
        <KEY NAME="outcomeareaid" TYPE="foreign" FIELDS="outcomeareaid" REFTABLE="outcome_areas" REFFIELDS="id" PREVIOUS="cmid"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="outcome_used_sets" COMMENT="Associates an outcome set with the course." PREVIOUS="outcome_used_areas" NEXT="outcome_attempts">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="courseid"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="outcomesetid"/>
        <FIELD NAME="outcomesetid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="courseid" NEXT="filter"/>
        <FIELD NAME="filter" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="Filter criteria for the outcome set." PREVIOUS="outcomesetid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="courseid"/>
        <KEY NAME="courseid" TYPE="foreign" FIELDS="courseid" REFTABLE="course" REFFIELDS="id" PREVIOUS="primary" NEXT="outcomesetid"/>
        <KEY NAME="outcomesetid" TYPE="foreign" FIELDS="outcomesetid" REFTABLE="outcome_sets" REFFIELDS="id" PREVIOUS="courseid"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="outcome_attempts" COMMENT="Track a user attempt against a content item within a particular activity." PREVIOUS="outcome_used_sets" NEXT="outcome_marks">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="outcomeusedareaid"/>
        <FIELD NAME="outcomeusedareaid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="outcomeusedareaid" NEXT="itemid"/>
        <FIELD NAME="itemid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="A way to identify an individual attempt" PREVIOUS="userid" NEXT="percentgrade"/>
        <FIELD NAME="percentgrade" TYPE="number" LENGTH="10" NOTNULL="true" SEQUENCE="false" DECIMALS="5" COMMENT="Percent grade earned on this attempt." PREVIOUS="itemid" NEXT="mingrade"/>
        <FIELD NAME="mingrade" TYPE="number" LENGTH="10" NOTNULL="true" SEQUENCE="false" DECIMALS="5" COMMENT="The minimum possible grade." PREVIOUS="percentgrade" NEXT="maxgrade"/>
        <FIELD NAME="maxgrade" TYPE="number" LENGTH="10" NOTNULL="true" SEQUENCE="false" DECIMALS="5" COMMENT="The maximum possible grade." PREVIOUS="mingrade" NEXT="rawgrade"/>
        <FIELD NAME="rawgrade" TYPE="number" LENGTH="10" NOTNULL="true" SEQUENCE="false" DECIMALS="5" COMMENT="The points earned, should be between the minimum and maximum grades." PREVIOUS="maxgrade" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="rawgrade" NEXT="timecreated"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Time created." PREVIOUS="timemodified"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="userid"/>
        <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id" PREVIOUS="primary"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="outcomeusedareaid_userid" UNIQUE="false" FIELDS="outcomeusedareaid, userid" NEXT="itemid"/>
        <INDEX NAME="itemid" UNIQUE="false" FIELDS="itemid" PREVIOUS="outcomeusedareaid_userid"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="outcome_marks" COMMENT="Graders will manually mark the user as having Met or Not Met an outcome." PREVIOUS="outcome_attempts" NEXT="outcome_marks_history">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="courseid"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="outcomeid"/>
        <FIELD NAME="outcomeid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="courseid" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="This is the user who is receiving the mark." PREVIOUS="outcomeid" NEXT="graderid"/>
        <FIELD NAME="graderid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="This is the user is doing the marking." PREVIOUS="userid" NEXT="result"/>
        <FIELD NAME="result" TYPE="int" LENGTH="2" NOTNULL="false" SEQUENCE="false" COMMENT="The marking result." PREVIOUS="graderid" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Time modified." PREVIOUS="result" NEXT="timecreated"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Time created." PREVIOUS="timemodified"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="courseid"/>
        <KEY NAME="courseid" TYPE="foreign" FIELDS="courseid" REFTABLE="course" REFFIELDS="id" PREVIOUS="primary" NEXT="outcomeid"/>
        <KEY NAME="outcomeid" TYPE="foreign" FIELDS="outcomeid" REFTABLE="outcome" REFFIELDS="id" PREVIOUS="courseid" NEXT="userid"/>
        <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id" PREVIOUS="outcomeid" NEXT="graderid"/>
        <KEY NAME="graderid" TYPE="foreign" FIELDS="graderid" REFTABLE="user" REFFIELDS="id" PREVIOUS="userid"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="outcome_marks_history" COMMENT="Keep track of past markings." PREVIOUS="outcome_marks" NEXT="outcome_awards">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="action"/>
        <FIELD NAME="action" TYPE="int" LENGTH="3" NOTNULL="true" SEQUENCE="false" COMMENT="Action taken on the mark" PREVIOUS="id" NEXT="outcomemarkid"/>
        <FIELD NAME="outcomemarkid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="action" NEXT="courseid"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="outcomemarkid" NEXT="outcomeid"/>
        <FIELD NAME="outcomeid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="courseid" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="This is the user who is receiving the mark." PREVIOUS="outcomeid" NEXT="graderid"/>
        <FIELD NAME="graderid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="This is the user is doing the marking." PREVIOUS="userid" NEXT="result"/>
        <FIELD NAME="result" TYPE="int" LENGTH="2" NOTNULL="false" SEQUENCE="false" COMMENT="The marking result." PREVIOUS="graderid" NEXT="timecreated"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Time created." PREVIOUS="result"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="courseid"/>
        <KEY NAME="courseid" TYPE="foreign" FIELDS="courseid" REFTABLE="course" REFFIELDS="id" PREVIOUS="primary" NEXT="outcomeid"/>
        <KEY NAME="outcomeid" TYPE="foreign" FIELDS="outcomeid" REFTABLE="outcome" REFFIELDS="id" PREVIOUS="courseid" NEXT="userid"/>
        <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id" PREVIOUS="outcomeid" NEXT="graderid"/>
        <KEY NAME="graderid" TYPE="foreign" FIELDS="graderid" REFTABLE="user" REFFIELDS="id" PREVIOUS="userid" NEXT="outcomemarkid"/>
        <KEY NAME="outcomemarkid" TYPE="foreign" FIELDS="outcomemarkid" REFTABLE="outcome_marks" REFFIELDS="id" PREVIOUS="graderid"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="outcome_awards" COMMENT="This preserves any outcomes earned by a user." PREVIOUS="outcome_marks_history">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="outcomeid"/>
        <FIELD NAME="outcomeid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="outcomeid" NEXT="timecreated"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Time created." PREVIOUS="userid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="outcomeid"/>
        <KEY NAME="outcomeid" TYPE="foreign" FIELDS="outcomeid" REFTABLE="outcome" REFFIELDS="id" PREVIOUS="primary" NEXT="userid"/>
        <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id" PREVIOUS="outcomeid"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>